  "license": "MIT",
  "scripts": {
    "start": "node dist/src/main/server.js",
    "dev": "NODE_ENV=dev ts-node src/main/server.ts",
    "build": "rimraf dist && tsc",
    "test": "NODE_ENV=sqlite_tests jest --passWithNoTests --silent --noStackTrace --runInBand --detectOpenHandles",
    "test:verbose": "NODE_ENV=sqlite_tests jest --passWithNoTests --runInBand",
    "test:unit": "NODE_ENV=sqlite_tests yarn test -- --watch -c jest-unit-config.js",
    "test:integration": "NODE_ENV=sqlite_tests yarn test -- --watch -c jest-integration-config.js",
    "test:staged": "NODE_ENV=sqlite_tests yarn test -- --findRelatedTests",
    "test:ci": "NODE_ENV=sqlite_tests yarn test -- --coverage"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.5",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.5",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mongodb": "^3.5.5",
    "@types/node": "^13.11.0",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "2",
    "eslint": "6",
    "eslint-config-standard-with-typescript": "^15.0.1",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "eslint-plugin-standard": "4",
    "git-commit-msg-linter": "^2.4.2",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "lint-staged": "^10.1.2",
    "mockdate": "^2.0.5",
    "rimraf": "^3.0.2",
    "sqlite3": "^4.1.1",
    "sucrase": "^3.13.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "express": "^4.17.3",
    "fast-glob": "^3.2.2",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.5",
    "pg": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.24",
    "validator": "^13.0.0"
  }
}
